{
    "name": "crazymeeks/dragonpay",
    "description": "A PHP library for Dragonpay payment gateway",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Dragonpay library",
        "Dragonpay PHP library",
        "Dragonpay PHP"
    ],
    "homepage": "https://github.com/crazymeeks/dragonpay",
    "authors": [
        {
            "name": "jclaud",
            "email": "jeffclaud17@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ixudra/curl": "^6.16"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "vlucas/phpdotenv": "^2.5",
        "phpunit/phpunit": "5.5.6",
        "phpstan/phpstan": "^0.12.92"
    },
    "autoload": {
        "psr-4": {
            "Crazymeeks\\": "src",
            "Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "crazymeeks/dragonpay",
                "version": "v3.2.8",
                "source": {
                    "url": "git://github.com/crazymeeks/dragonpay",
                    "type": "git",
                    "reference": "v3.2.8"
                },
                "autoload": {
                    "psr-4": {
                        "Crazymeeks\\": "src"
                    }
                }
            }
            
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "v3.2.x-dev"
        }
    }
}
